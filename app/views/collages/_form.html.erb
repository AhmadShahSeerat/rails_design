<%= form_with(model: model) do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :description %>
    <%= f.text_field :description %><br>
    <h3> <%= form_title %></h3>
    <%= f.fields_for :designs do |i| %>
      <%= i.label :title %>
      <%= i.text_field :title %><br>
      <%= i.label :description %>
      <%= i.text_field :description %><br>
      <%= i.label :design_url %>
      <%= i.text_field :design_url %><br>
      <hr>
    <% end %>
  
    
    <%= f.submit button_name %>
<% end %>